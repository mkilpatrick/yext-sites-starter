{
  "name": "yext-sites-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "yss dev",
    "build": "yss build",
    "directbuild": "node --experimental-specifier-resolution=node node_modules/.bin/yss build",
    "cibuild": "npm run smartlingpush && npm run smartlingpull && npm run directbuild",
    "preview": "yss preview",
    "format": "prettier --write \"src/**/*.tsx\" \"entry.js\"",
    "lint": "eslint .",
    "clean": "rm -rf .artifact-output .yext/ dist/ localData sites-rendered-output",
    "smartlingpush": "node src/i18n/smartling/index.js push",
    "smartlingpull": "node src/i18n/smartling/index.js pull"
  },
  "engines": {
    "node": ">=17"
  },
  "dependencies": {
    "@yext/yext-sites-scripts": "mkilpatrick/yext-sites-scripts#0482ef34613c4b5366cea719339f3636427c7990",
    "cross-fetch": "^3.1.5",
    "i18next": "^21.6.14",
    "i18next-conv": "^12.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.16.2",
    "smartling-api-sdk-nodejs": "^1.2.4",
    "typescript": "^4.6.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.10",
    "autoprefixer": "^10.4.4",
    "postcss": "^8.4.12",
    "prettier": "^2.6.1",
    "tailwindcss": "^3.0.23"
  }
}
